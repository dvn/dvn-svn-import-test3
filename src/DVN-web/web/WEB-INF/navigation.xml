<!-- 
  Navigation rules from SubsettingPage.jsp to RequestSummaryPage.jsp
  ///////////////// start ////////////////
-->
  <navigation-rule>
    <from-view-id>/subsetting/SubsettingPage.jsp</from-view-id>
    
<!-- download request -->
    <navigation-case>
      <from-action>#{AnalysisPage.dwnldAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/AnalysisResultsPage.jsp</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-action>#{AnalysisPage.dwnldAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.jsp</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{AnalysisPage.dwnldAction}</from-action>
      <from-outcome>download</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.jsp</to-view-id>
    </navigation-case>
    
<!-- eda request -->
    <navigation-case>
      <from-action>#{AnalysisPage.edaAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/AnalysisResultsPage.jsp</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{AnalysisPage.edaAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.jsp</to-view-id>
    </navigation-case>
    
<!-- recode rquest-->
    <navigation-case>
      <from-action>#{AnalysisPage.recodeAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.jsp</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{AnalysisPage.recodeAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.jsp</to-view-id>
    </navigation-case>

<!-- advanced statistics request-->

    <navigation-case>
      <from-action>#{AnalysisPage.advStatAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/AnalysisResultsPage.jsp</to-view-id>
    </navigation-case>
    
    <!-- advanced statistics request-->
    <navigation-case>
      <from-action>#{AnalysisPage.advStatAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.jsp</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-action>#{AnalysisPage.gotoEditVariableAction}</from-action>
      <from-outcome>editVariable</from-outcome>
      <to-view-id>/study/EditVariablePage.jsp</to-view-id>
    </navigation-case>   

  </navigation-rule> 
  
  
  
<!-- 
  Navigation rules from SubsettingPage.jsp to RequestSummaryPage.jsp
  ///////////////// end ////////////////
-->
<navigation-rule>
    <navigation-case>
        <from-outcome>
            exceptionNavigation
        </from-outcome>
        <to-view-id>
            /ExceptionHandler
        </to-view-id>
    </navigation-case>
</navigation-rule>
    
